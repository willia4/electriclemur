{
    "compileOnSave": false,
    "compilerOptions": {
        "rootDir": "src",
        "outDir": "./dist",
        "sourceMap": true,
        "target": "es5",

        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "moduleResolution": "node",
        "lib": ["es2015", "dom"],
        "baseUrl": "./",
        "typeRoots": [
            "node_modules/@types"
        ],
        "types": [
            "jquery",
            "jqueryui",
            "node"
        ],
        "paths": {
            "src/*": ["src/*"]
        }
    },
    "exclude": [
        "node_modules",
        "dist",
        "build"
    ],
    "filesGlob": [
        "./src/**/*.ts",
        "./test/**/*.ts",
        "./typings/index.d.ts",
        "./custom_typings/**/*.d.ts",
        "./node_modules/@types/**/*.d.ts"
    ],
    "atom": {
        "rewriteTsconfig": false
    }
    }